
package com.joker.singleton.lazy;

/**
 * 单例模式
 */
public class Singleton {

  private static Singleton singleton;

  private Singleton() {}

  public Singleton getInstance() {
    if (null == singleton) {
      // 双重检查
      synchronized (Singleton.class) {
        if (null == singleton) {
          singleton = new Singleton();
        }
      }
    }
    return singleton;
  }
}
